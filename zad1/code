    "# URL główny\n",
    "URL = \"https://en.wikipedia.org/wiki/Academy_Award_for_Best_Picture\"\n",
    "\n",
    "response = requests.get(URL)\n",
    "\n",
    "soup = BeautifulSoup(response.text, 'html')"
