   "source": [
    "for row in soup.find_all('tr', style={'background:#FAEB86'}):\n",
    "    #print(row)\n",
    "    for film in row.find_all('a', href=True):\n",
    "        urls.append('https://en.wikipedia.org'+ film['href'])\n",
    "        titles.append(film['title'])"
   ]
