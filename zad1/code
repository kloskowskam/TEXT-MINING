   "source": [
    "for i in range(len(urls)):\n",
    "    #weź co drugi element, bo tylko on jest tytułem filmu\n",
    "    if i%2 == 0:\n",
    "        #print(i)\n",
    "        response = requests.get(urls[i])\n",
    "        soup = BeautifulSoup(response.text, 'html')\n",
    "\n",
    "        # OPIS FABUŁY\n",
    "        # wyszukaj span o identyfikatorze id = \"Plot\"\n",
    "        idx = soup.find(id=\"Plot\")\n",
    "        #przejdź przez węzły nadrzędne rodzeństwa, zbierając ich tekst, aż dojdziesz do następnego nagłówka H2\n",
    "        try:\n",
    "            for wezel in idx.parent.nextSiblingGenerator():\n",
    "                if wezel.name == \"h2\":\n",
    "                    break\n",
    "                if hasattr(wezel, \"text\"):\n",
    "                    plots.append(wezel.text)\n",
    "            #ZNAJDŹ REŻYSERA       \n",
    "            infobox = soup.find('table', {'class': 'infobox'})\n",
    "            #jest w 3 tr\n",
    "            third_tr = infobox.find_all('tr')[2]\n",
    "            # w pierwszym a\n",
    "            first_a = third_tr.find('a')\n",
    "            # w tytule\n",
    "            director = first_a['title']\n",
    "            names.append(director)\n",
    "            print(i)\n",
    "        except:\n",
    "            pass"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 188,
   "metadata": {},
   "outputs": [],
   "source": [
    "for row in soup.find_all('table', {'class': 'wikitable sortable'}):\n",
    "\n",
    "    for film in row.find_all('tr'):\n",
    "    \n",
    "        for td in film.find_all('td', style=\"text-align:center\"):\n",
    "            print(td.get_text())\n",
    "            years.append(td.get_text())\n",
    "\n",
    "        "
   ]
